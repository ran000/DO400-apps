pipeline {
    agent {
        node {
            label 'nodejs'
        }
    }
    stages {
        stage('Install dependencies') {
            steps {
                dir ('exchang-cli') {
                    sh 'npm install'
                }
            }
        }
        stage('Build') {
            steps {
                dir ('exchang-cli') {
                    sh 'npm run build'
                }
            }
        }
    }
}

